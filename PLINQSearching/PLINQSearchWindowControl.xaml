<UserControl x:Class="PLINQSearching.PLINQSearchWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             Background="{DynamicResource VsBrush.Window}"
             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Name="MyToolWindow">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Margin="10" HorizontalAlignment="Center">PLINQSearchWindow</TextBlock>
            <Button Content="IndexOf" Click="IndexOf_Click" Width="120" Height="22" Name="button1" Margin="90,0"/>
            <Button Content="Contains" Margin="90,0" Click="ContainsSearch_Click"/>
            <Button Content="Boyer-Moore" Margin="90,0"/>
            <Button Content="Naive" Margin="90,0"/>
            <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="TextBox"/>
        </StackPanel>
    </Grid>
</UserControl>
